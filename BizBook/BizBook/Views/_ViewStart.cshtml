@{
    if (this.User.IsInRole("Consumer"))
    {
        Layout = "~/Views/Shared/_LayoutCustomer.cshtml";
    }
    else if (this.User.IsInRole("Business"))
    {
        Layout = "~/Views/Shared/_BusinessProfile.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
}